{
    "version": 2,
    "builds": [
      {
        "src": "app/api/**/*",
        "use": "@vercel/node"
      },
      {
        "src": "app/**/*",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/app/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/app/$1"
      }
    ],
    "public": "public"
  }
  